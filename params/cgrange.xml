<?xml version="1.0" encoding="utf-8"?>
<form>
	<fields name="fieldparams">
		<fieldset name="fieldparams" addfieldpath="/plugins/fields/cgrange/src/Form/Field">
			<field
				name="min"
				type="number" default="1"
				label="PLG_FIELDS_CGRANGE_MIN_LABEL"
				description="PLG_FIELDS_CGRANGE_MIN_DESC"
			/>
			<field
				name="max"
				type="number" default="10"
				label="PLG_FIELDS_CGRANGE_MAX_LABEL"
				description="PLG_FIELDS_CGRANGE_MAX_DESC"
			/>
			<field
				name="step"
				type="number" default="1"
				label="PLG_FIELDS_CGRANGE_STEP_LABEL"
				description="PLG_FIELDS_CGRANGE_STEP_DESC"
			/>
			<field
				name="width"
				type="text" default="auto"
				label="PLG_FIELDS_CGRANGE_WIDTH_LABEL"
				description="PLG_FIELDS_CGRANGE_WIDTH_DESC"
			/>
			<field
				name="limits"
				type="radio" default="show" class="btn-group btn-group-yesno"
				label="PLG_FIELDS_CGRANGE_LIMITS_LABEL"
				description="PLG_FIELDS_CGRANGE_LIMITS_DESC">
					<option value="show">JYES</option> 
					<option value="hide">JNO</option> 
			</field>
		</fieldset>
	</fields>
</form>
